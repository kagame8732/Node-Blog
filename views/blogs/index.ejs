<html lang="en">
  <%- include('../partial/head.ejs') %>
  <body>
    <%- include('../partial/nav.ejs') %>
    <div class="blogs content">
      <h2>All Blog</h2>
      <% if(blogs.forEach((blog)=>{%>
      <a class="blogs" href="/blogs/<%= blog._id%>">
        <h1><%=blog.title %></h1>
        <p><%= blog.snippet%></p>
      </a>
      <% })) %>
    </div>
    <%- include('../partial/footer.ejs') %>
  </body>
</html>
